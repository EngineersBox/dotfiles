# dmenu
cmd + space : open -a Xquartz && /usr/bin/env bash -l -c "sh $HOME/.config/dmenu/apps.sh"    # App launcher
cmd + shift - l : open -a Xquartz && /usr/bin/env bash -l -c "sh ~/Dotfiles/dmenu/logout.sh" # Logout menu
cmd + shift - n : open -a Xquartz && /usr/bin/env bash -l -c "sh ~/Dotfiles/dmenu/notes.sh"  # Notes
