# dmenu
cmd - space : /usr/bin/env bash -l -c "sh ~/.config/de_menu/apps.sh"    # App launcher
cmd + shift - q : /usr/bin/env bash -l -c "sh ~/.config/de_menu/logout.sh" # Logout menu
cmd + shift - n : /usr/bin/env bash -l -c "sh ~/.config/de_menu/notes.sh"  # Notes
cmd + shift - e : /usr/bin/env bash -l -c "sh ~/.config/de_menu/calc.sh" # Calculator
